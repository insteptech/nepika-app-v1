<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Nepika</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Nepika</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSCameraUsageDescription</key>
	<string>Nepika uses your camera to perform advanced skin analysis and provide personalized skincare recommendations. Your photos are processed securely and never stored without your permission.</string>
	<key>NSContactsUsageDescription</key>
	<string>Nepika may use SMS autofill to automatically fill OTP codes for your convenience.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Nepika uses your location to provide relevant skincare recommendations based on your local climate and environmental conditions.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Nepika needs permission to save your skin analysis photos to your photo library.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Nepika may access your photo library to save analysis results and skincare progress photos for your personal tracking.</string>
	<key>NSUserNotificationsUsageDescription</key>
	<string>Nepika sends you personalized skincare reminders to help you maintain a consistent routine and achieve your skin goals.</string>
	<key>NSUserActivityTypes</key>
	<array>
		<string>com.apple.developer.associated-domains</string>
	</array>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.assisted.nepika.background.processing</string>
		<string>com.assisted.nepika.background.refresh</string>
	</array>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>processing</string>
		<string>fetch</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
</dict>
</plist>
